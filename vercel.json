{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "crons": [
    {
      "path": "/api/cron-partner-tips",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/cron-monthly-report",
      "schedule": "0 8 1 * *"
    }
  ]
}